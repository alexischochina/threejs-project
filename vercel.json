{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".output/public",
  "routes": [
    {
      "src": "/_nuxt/(.*\\.js)",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt/(.*\\.mjs)",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt/(.*\\.css)",
      "headers": {
        "content-type": "text/css",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 